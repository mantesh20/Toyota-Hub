{
  "version": 2,
  "git": {
    "deploymentEnabled": {
      "toyota-hub": true
    }
  },
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/frontend/build/$1" },
    { "src": "/", "dest": "/frontend/build/index.html" }
  ]
}